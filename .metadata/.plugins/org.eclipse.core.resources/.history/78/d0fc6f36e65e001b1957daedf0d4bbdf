package Javaconcepts;

public class Interview 
{
	
	

	public static void main(String[] args) 
	{
		
		//1,2,3,5,7,
		
		int a=1; 
		
		int b=2;
		
		int fact=1;
		
		int n=1;
		
		System.out.println(a+b);
		
		int c=a+b;
		
		for(int i=3;i<=n;i++)
		{
		
		fact=fact*c;
		
		System.out.println(fact);
		
		
		}
		
		
	}
		
		

}
