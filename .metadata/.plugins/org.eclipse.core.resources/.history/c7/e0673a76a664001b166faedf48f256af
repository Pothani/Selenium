package Javaconcepts;

public class Interview 
{
	public static void main(String[] args) 
	{
		
		String str="Bhargav is good";
		String[] s2=str.split(" ");
		
		for(int i=0;i<=3;i++)
		{
			char[] ch=s2[i].toCharArray();
			for(int j=ch.length-1;j>=0;j--)
			{
				System.out.println(ch[j]);
				
			}
			
			System.out.println("  ");
		}
		
		
		}
		
		
	}
		
		


