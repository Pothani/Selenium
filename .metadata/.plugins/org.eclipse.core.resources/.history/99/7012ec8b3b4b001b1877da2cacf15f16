		String s1="The rain has started here selenium s uytrs";
		
		//System.out.println(s1.length());//It will give length of string =from 1
		
		System.out.println(s1.indexOf("s"));//1st occrance
		
		System.out.println(s1.indexOf("s", s1.indexOf("s")+1));//2nd ocuurance
		
		System.out.println(s1.indexOf('s', s1.indexOf('s', s1.indexOf('s')+1)+1));//3rd occurance
		
		System.out.println(s1.indexOf('s', s1.indexOf('s',s1.indexOf('s',s1.indexOf('s')+1)+1)+1));
		
		System.out.println(s1.indexOf('s', s1.indexOf('s',s1.indexOf('s',s1.indexOf('s',s1.indexOf('s')+1)+1)+1)+1));
		
		
