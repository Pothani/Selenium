package Com.Pages;

	class A
	{
		public void demo() 
		{
		    System.out.println("Firstmethod");
		}
		
	}
	class B extends A
	{
		public void demo() 
		{
			super.demo();
		    System.out.println("secondtmethod");
		}
	}
	
	public class Override 
	{
	public static void main(String[] args) {
		
         B c=new B();
         c.demo();
		
	}
	
}
