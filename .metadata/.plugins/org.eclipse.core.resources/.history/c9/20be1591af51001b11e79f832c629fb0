package Practisejava;

import java.util.Scanner;

public class Primenumber 

{
	
	public void prime()
	{
		Scanner s= new Scanner(System.in);
		System.out.println("Enter the number");
		int a= s.nextInt();
		int temp=0;
		
		for(int i=2;i<a-1;i++) {
			
			 temp=temp+1;
		}
		
		
	}
	
	public final static void main(String[] args) 
	{
		
	}

}
