package Javaconcepts;

import java.util.Scanner;

public class Interview 
{
	public static void main(String[] args) 
	{
		Scanner s=new Scanner(System.in);
		System.out.println("Enter the number");
		int n=s.nextInt();
		
		int a=0,b=1;
		for(int i=2;i>=n;i++)
		{
			int c=a+b;
			System.out.println(c);
			a=b;
			b=c;
		}
		
	}
		
}
		
		


