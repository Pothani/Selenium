package Practisejava;

import java.util.Scanner;

public class Fibnociseries 
{

	public static void main(String[] args)
	{
		Scanner s= new Scanner(System.in);
		System.out.println("Enter the number : ");
		int g=s.nextInt();
		int a=0,b=1;
		for(int i=1;i<=g;i++)
		{
		int c=a+b;
		
		System.out.println(c);
		a=b;
		b=c;
		}
	}

}
