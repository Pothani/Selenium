package Javaconcepts;

import java.util.Scanner;

public class Interview 
{
	public static void main(String[] args) 
	{
		Scanner s=new Scanner(System.in);
		System.out.println("Enter the number");
		int n=s.nextInt();
		int fact=1;
		
		for(int i=0;i<=n;i++) {
			fact=fact*i;
		}
		
		System.out.println(fact);
		
		
	}
		
}
		
		


