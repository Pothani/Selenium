package Javaconcepts;

public class StringManipulations {

	public static void main(String[] args) {
		
		String s1="Happy New Year 2021 Y";
		
	    System.out.println(s1.length());//It will start from 1
	    
	    System.out.println(s1.indexOf("Y"));//It will start fromm 0
	    
	    System.out.println(s1.indexOf("Y",s1.indexOf("Y")+1));
	    	

	}

}
