package Javaconcepts;

public class Interview 
{
	public static void main(String[] args) 
	{
		
		String s1="Bhargav is a good guy";
		String[] s2=s1.split(" ");
		
		
		for(int i=0;i<=4;i++)
		{
	    char[] ch=s2[i].toCharArray();
	    
	    for(int j=ch.length-1;j>=0;j--)
	    {
	    	
	    	System.out.print(ch[j]);
	    }
	    
		}
		
	}
		
}
		
		


