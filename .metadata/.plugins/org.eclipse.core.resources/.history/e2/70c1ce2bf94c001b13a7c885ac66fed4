package Javaconcepts;

public class childclass s {
	
	//Aquarig the properties of another classs
	//To do that we need to create a parent and child 
	//by using extends keyword
	//The ground rule of inheritance is should create the child class object

	public void add() 
	{
		System.out.println("This is child class object");
	}
	public static void main(String[] args) {
		
		childclass ch= new childclass();
		ch.demo();
		ch.add();
		

	}

}
